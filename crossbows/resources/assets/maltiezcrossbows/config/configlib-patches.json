{
  "version": 2,
  "patches": {
    "float": {
      "maltiezcrossbows:itemtypes/crossbow.json": {
        "attributes/BoltDamageMultiplierByType/@@*": "value * damage_multiplier",
        "attributes/AimingByType/@@*/AimDifficulty": "aiming_difficulty",
        "attributes/DispersionMOAByType/@@*/0": "dispersion * value",
        "attributes/DispersionMOAByType/@@*/1": "dispersion * value",
        "attributes/ReloadAnimationSpeed": "reload_speed"
      },
      "maltiezcrossbows:itemtypes/crossbow-repeating.json": {
        "attributes/BoltDamageMultiplierByType/@@*": "value * damage_multiplier",
        "attributes/AimingByType/@@*/AimDifficulty": "aiming_difficulty",
        "attributes/DispersionMOA/0": "dispersion * value",
        "attributes/DispersionMOA/1": "dispersion * value",
        "attributes/ReloadAnimationSpeed": "reload_speed"
      }
    },
    "integer": {
      "maltiezcrossbows:itemtypes/crossbow-repeating.json": {
        "attributes/MagazineSize": "magazine_size",
        "durability": "value * durability_repeating / 400"
      },
      "maltiezcrossbows:itemtypes/crossbow.json": {
        "durabilityByType/*-simple-*": "value * durability_simple / 240",
        "durabilityByType/*-stirrup-*": "value * durability_stirrup / 750",
        "durabilityByType/*-latch-*": "value * durability_latch / 750",
        "durabilityByType/*-goatsfoot-*": "value * durability_goatsfoot / 900",
        "durabilityByType/*-windlass-*": "value * durability_windlass / 600"
      }
    },
    "boolean": {
      "maltiezcrossbows:itemtypes/crossbow.json": {
        "attributes/CancelReloadOnInAirByType/*-stirrup-*": "cancel_reload_in_the_air",
        "attributes/CancelReloadMountedByType/*-stirrup-*": "cancel_reload_while_mounted",
        "attributes/CancelReloadOnInAirByType/*-windlass-*": "cancel_reload_in_the_air",
        "attributes/CancelReloadMountedByType/*-windlass-*": "cancel_reload_while_mounted"
      }
    },
    "other": {
      "maltiezcrossbows:recipes/grid/bow.json": {
        "0/requiresTrait": "trait_requirement"
      }
    }
  },
  "settings": [
    {
      "code": "aiming_cursor_type",
      "type": "string",
      "ingui": "maltiezcrossbows:setting-cursortype",
      "comment": "maltiezcrossbows:setting-comment-cursortype",
      "default": "Fixed",
      "clientSide": true,
      "values": [
        "None",
        "Vanilla",
        "Fixed",
        "Moving"
      ]
    },
    {
      "code": "aiming_difficulty",
      "type": "float",
      "ingui": "maltiezcrossbows:setting-aiming_difficulty",
      "default": 1.0,
      "range": {
        "min": 0.0,
        "max": 2.0
      }
    },
    {
      "code": "reload_speed",
      "type": "float",
      "ingui": "maltiezcrossbows:setting-reload_speed",
      "default": 1.0,
      "range": {
        "min": 0.3,
        "max": 3.0
      }
    },
    {
      "code": "damage_multiplier",
      "type": "float",
      "ingui": "maltiezcrossbows:setting-damage",
      "default": 1.0,
      "range": {
        "min": 0.1,
        "max": 5.0
      }
    },
    {
      "code": "trait_requirement",
      "type": "other",
      "ingui": "maltiezcrossbows:setting-trait",
      "default": "Required",
      "mapping": {
        "Required": "bowyer",
        "Not Required": null
      }
    },
    {
      "code": "dispersion",
      "type": "float",
      "ingui": "maltiezcrossbows:setting-dispersion",
      "default": 1.0,
      "range": {
        "min": 0.0,
        "max": 5.0
      }
    },
    {
      "code": "magazine_size",
      "type": "integer",
      "ingui": "maltiezcrossbows:setting-magazine_size",
      "default": 5
    },
    {
      "code": "cancel_reload_in_the_air",
      "type": "boolean",
      "ingui": "maltiezcrossbows:setting-cancel_reload_in_the_air",
      "default": true
    },
    {
      "code": "cancel_reload_while_mounted",
      "type": "boolean",
      "ingui": "maltiezcrossbows:setting-cancel_reload_while_mounted",
      "default": true
    },
    {
      "type": "separator",
      "title": "maltiezcrossbows:setting-title-durabilities",
      "collapsible": true
    },
    {
      "code": "durability_simple",
      "type": "integer",
      "ingui": "maltiezcrossbows:setting-durability-simple",
      "default": 240,
      "range": {
        "min": 1
      }
    },
    {
      "code": "durability_repeating",
      "type": "integer",
      "ingui": "maltiezcrossbows:setting-durability-repeating",
      "default": 400,
      "range": {
        "min": 1
      }
    },
    {
      "code": "durability_stirrup",
      "type": "integer",
      "ingui": "maltiezcrossbows:setting-durability-stirrup",
      "default": 750,
      "range": {
        "min": 1
      }
    },
    {
      "code": "durability_latch",
      "type": "integer",
      "ingui": "maltiezcrossbows:setting-durability-latch",
      "default": 750,
      "range": {
        "min": 1
      }
    },
    {
      "code": "durability_goatsfoot",
      "type": "integer",
      "ingui": "maltiezcrossbows:setting-durability-goatsfoot",
      "default": 900,
      "range": {
        "min": 1
      }
    },
    {
      "code": "durability_windlass",
      "type": "integer",
      "ingui": "maltiezcrossbows:setting-durability-windlass",
      "default": 600,
      "range": {
        "min": 1
      }
    }
  ]
}